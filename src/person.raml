#%RAML 1.0 Library
types:
  Address:
    type: object
    properties:
      street_address: string
      city: string
      state?: string
      country: string
      planet?: string
  Human:
    type: object
    properties:
      first_name: string
      last_name: string
      age?:
        type: integer
        description: Age in years
      home_address: Address
      work_address?: Address
      other_addresses?: array
    description: Human Schema

